{ 
  "rewrites": [
    { "source": "/login", "destination": "/login.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/stalls", "destination": "/stalls.html" },
    { "source": "/add-stall", "destination": "/add-stall.html" },
    { "source": "/stall-details", "destination": "/stall-details.html" },
    { "source": "/stall-details/:id", "destination": "/stall-details.html" },
    { "source": "/css/(.*)", "destination": "/css/$1" },
    { "source": "/js/(.*)", "destination": "/js/$1" },
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
